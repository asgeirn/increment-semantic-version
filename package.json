{
  "name": "@asgeirn/increment-semantic-version",
  "version": "0.0.0",
  "private": true,
  "description": "TypeScript template action",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "all": "run-s build format lint package test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/asgeirn/increment-semantic-version.git"
  },
  "keywords": [
    "actions",
    "semver"
  ],
  "author": "asgeirn",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.11",
    "@typescript-eslint/parser": "^5.45.1",
    "@vercel/ncc": "^0.36.0",
    "eslint": "^8.29.0",
    "eslint-plugin-github": "^4.6.0",
    "eslint-plugin-jest": "^27.1.6",
    "jest": "^29.3.1",
    "js-yaml": "^4.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "2.8.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.4"
  }
}
